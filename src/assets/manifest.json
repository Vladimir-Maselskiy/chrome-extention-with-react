{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "SilentWeb",
  "description": "Tools for remove web content",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "A-React-Title",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
